{
    "compilerOptions": {
        "jsx": "react-jsx",
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "typeRoots": [ "./assets/types/", "./node_modules/@types/" ]
    },
    "include": [ "./**/*.ts", "./**/*.tsx" ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
